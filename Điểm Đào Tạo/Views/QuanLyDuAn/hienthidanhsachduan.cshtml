@model List<duan>
<h1 style="text-align:center;color:#808080;margin-top:50px">Danh sách dự án</h1>
 
    @if (Model != null)
    {
        @if (Model.Count() > 0)
        {  
             
 
            <table class="table table-bordered" style="text-align:center;margin-top:50px" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th>Tên dự án </th>
                        <th>Mã dự án </th>
                       

                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td style="text-align:center">@item.tenduan</td>
                            <td style="text-align:center">@item.maduan</td>
                          


                        </tr>
                    }

                                        
                                        
                                    </tbody>


                                </table>}}
   