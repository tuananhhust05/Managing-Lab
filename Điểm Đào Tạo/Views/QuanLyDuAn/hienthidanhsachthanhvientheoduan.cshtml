@model IEnumerable<ChiTietDuAn>
@{
    Layout = null;
}


@if(Model!=null)
{
    <hr/>
    <table style="border-collapse:collapse;width:1000px; margin:auto;margin-top:50px; border:1px solid black; border-spacing:10px;background-color: #E6E6FA">
        <tr>
              <td style="border:1px solid black;padding:10px;">ID</td>
              <td style="border:1px solid black;padding:10px;">LabID</td>
              <td style="border:1px solid black;padding:10px;">Tên</td>
              <td style="border:1px solid black;padding:10px;">Tên dự án </td>
              <td style="border:1px solid black;padding:10px;">Chức vụ </td>
              <td style="border:1px solid black;padding:10px;">Bắt đầu</td>
