@model IEnumerable<ChiTietDuAn>

<form style="margin-top:100px;margin-left:450px;" asp-controller="QuanLyDuAn" asp-action="timkiembangex"method="post"enctype="multipart/form-data">
    <input type="file" name="file"/>
    <hr/>
    <button type="submit">Tìm kiếm</button>
</form>
<button style="text-align:center;margin-top:25px;margin-left:450px;background-color:#6495ED;width:300px;height:50px;color:white;border-radius: 4px;">  
         <a asp-action="xuatketquatimraex"asp-controller="QuanLyDuAn" class="btnlogin"style="text-decoration: none;color:white;">Xuất danh sách tìm được bằng file excel</a>
     </button>

@if(Model!=null)
{
    <hr/>
   <table style="border-collapse:collapse;width:1000px; margin:auto;margin-top:50px; border:1px solid black; border-spacing:10px;background-color: #E6E6FA">
        <tr>
              <td style="border:1px solid black;padding:10px;">ID</td>
              <td style="border:1px solid black;padding:10px;">LabID</td>
              <td style="border:1px solid black;padding:10px;">Tên</td>
              <td style="border:1px solid black;padding:10px;">Tên dự án </td>
              <td style="border:1px solid black;padding:10px;">Chức vụ </td>
              <td style="border:1px solid black;padding:10px;">Bắt đầu</td>
              <td style="border:1px solid black;padding:10px;">Kết thúc</td>
              <td style="border:1px solid black;padding:10px;">Đánh giá</td>
             
        </tr>
        @foreach(var item in Model)
        {
            <tr>
                <td style="border:1px solid black; text-align: center;padding:10px;background-color: #B0E0E6">@item.ID</td>
                <td style="border:1px solid black; text-align: center;padding:10px;background-color: #B0E0E6">@item.LabID</td>
                <td style="border:1px solid black; text-align: center;padding:10px;background-color: #B0E0E6">@item.Ten</td>
                <td style="border:1px solid black; text-align: center;padding:10px;background-color: #B0E0E6">@item.Tenduan</td>
                <td style="border:1px solid black; text-align: center;padding:10px;background-color: #B0E0E6">@item.Chucvu</td>
                <td style="border:1px solid black; text-align: center;padding:10px;background-color: #B0E0E6">@item.BatDau</td>
                <td style="border:1px solid black; text-align: center;padding:10px;background-color: #B0E0E6">@item.KetThuc</td>
                <td style="border:1px solid black; text-align: center;padding:10px;background-color: #B0E0E6">@item.Danhgia</td>
                
            </tr>
        }
    </table>
}

