<h1 style="text-align:center;margin-top:100px;">@ViewBag.Name </h1>


     <!-- show dữ liệu-->
     @if(Model!=null){
     @if(Model.Count() > 0)
 {
        @foreach (var member in Model)
        {
            <hr />
