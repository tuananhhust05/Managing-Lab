@model List<HienThiThongTinNguoiDungTkUser>
@{
   
    Layout = "~/Views/Shared/layoutTkUser.cshtml";
}

 <style>

#tendangnhap{
    text-align: center;
    margin-left: 450px;
}
.form-group{
   
    
    margin-top:10px;

}
.btn btn-default{
    color: aqua;
}
.formlogin{
    margin-top:100px;
