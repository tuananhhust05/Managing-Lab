@model List<diemythuc>
<form style="margin-top:100px;margin-left:450px;" asp-controller="DiemYThuc" asp-action="capnhatdiemythucbangex"method="post"enctype="multipart/form-data">
    <input type="file" name="file"/>
    <hr/>
    <button type="submit">Cập nhật</button>
</form>



<h1 style="text-align:center;color:#808080">Điểm sau khi cập nhật </h1>

    @if (Model != null)
    {
        @if (Model.Count() > 0)
        {  
             
 
            <table class="table table-bordered" style="text-align:center;margin-top:50px" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th>LabID </th>
                        <th>Tên </th>
                       <th>Điểm Ý Thức </th>

                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td style="text-align:center">@item.LabID</td>
                            <td style="text-align:center">@item.Ten</td>
                            <td style="text-align:center">@item.DiemYThuc</td>


                        </tr>
                    }

                                        
                                        
                                    </tbody>


                                </table>}}
